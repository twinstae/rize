  "scripts": {
    "build:tauri": "vite build --config='vite.config.tauri.js'",
    "build:cap": "vite build --config='vite.config.capacitor.js' && npx cap copy",
    "build": "tsc && concurrently \"yarn build:tauri\" \"yarn build:cap\"",
    "dev": "concurrently -k \"vite --config='vite.config.tauri.js'\" \"tauri dev\"",
    "test": "vitest --config=vitest.config.js run",
    "test:watch": "vitest --config=vitest.config.js",
    "test:cov": "vitest run --config=vitest.config.js --coverage=true",
    "stryker": "stryker run --fileLogLevel trace --logLevel debug",
    "tauri:build": "tauri dev --debug",
    "lint": "eslint --ext ts,tsx src",
    "ionic:build": "npm run build",
    "e2e:ct": "playwright test -c playwright-ct.config.ts"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "yarn@3.2.2"
