  "scripts": {
    "build:tauri": "vite build --config=vite.config.tauri.js",
    "build:cap": "vite build --config=vite.config.capacitor.js && npx cap copy",
    "build": "tsc && concurrently \"npm run build:tauri\" \"npm run build:cap\"",
    "dev:web": "vite --config=vite.config.web.js",
    "dev:web:cov": "npm run dev:web",
    "serve:tauri": "nyc --silent vite --config=vite.config.tauri.js",
    "dev:tauri": "tauri dev",
    "test": "vitest --config=vitest.config.js run",
    "test:watch": "vitest --config=vitest.config.js",
    "test:cov": "nyc --silent vitest --config=vitest.config.js run",
    "stryker": "stryker run --fileLogLevel trace --logLevel debug",
    "tauri": "tauri",
    "tauri:build": "tauri build",
    "lint": "eslint --ext ts,tsx src",
    "ionic:serve": "nyc --silent vite serve --config=vite.config.capacitor.js",
    "ionic:build": "npm run build",
    "ionic:android": "npx cap run android --livereload --external",
    "e2e": "playwright test",
    "e2e:headed": "playwright test --headed",
    "e2e:debug": "playwright test --headed --debug",
    "e2e:cov": "npm run e2e",
    "cov:report": "nyc report --reporter=html --reporter=text",
    "cov:tauri": "node ./src/test/renderTestResult.js",
    "cov": "npm run test:cov && npm run e2e:cov && npm run cov:tauri && npm run cov:report"
  },
  "browserslist": {
