    "build": "tsc && concurrently \"pnpm build:tauri\" \"pnpm build:cap\"",
    "dev:web": "vite --config=vite.config.web.js",
    "dev:web:cov": "pnpm dev:web",
    "serve:tauri": "nyc --silent vite --config=vite.config.tauri.ts",
